  "engines": {
    "node": ">=16.13.0"
  },
  "devDependencies": {
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.4",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.2"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.8.3",
    "@u4/opencv4nodejs": "^6.4.1",
    "dotenv": "^16.0.3",
    "google-protobuf": "^3.21.2",
