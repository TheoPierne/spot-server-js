{
  "name": "spot-server-js",
  "version": "1.0.0",
  "description": "A simple server to simulate a Spot's robot for testing purpose.",
  "main": "src/server.js",
  "files": [
    "src"
  ],
  "scripts": {
    "start": "node src/server.js",
    "dev": "set NODE_ENV=development && npm start",
    "prod": "set NODE_ENV=production && npm start",
    "lint": "eslint --config .eslintrc.json .",
    "build": "build-opencv --version 4.6.0 --nobuild rebuild"
  },
